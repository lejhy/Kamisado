        </ownedParameter>
        <language>java</language>
        <body>player1 = new Player(&quot;1&quot;, &quot;bottom&quot;);
		player2 = new Player(&quot;2&quot;, &quot;top&quot;);
		board = new Board();
		gameOver = false;
		scanner = new Scanner(System.in);
	</body>
